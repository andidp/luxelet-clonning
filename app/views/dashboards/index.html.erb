        <h4>Partner</h4>
        <span class="c_user"><strong><%= current_user %></strong></span>
        <ol class="list-unstyled">
          <li>
            <%= link_to "Create Booking", new_enquiry_path %>
          </li>
        </ol>
      </div>
      <div class="sidebar-module">
